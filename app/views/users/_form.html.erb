<%= form_for @user do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :photo %>
  <%= f.text_field :photo %>
  <br>

  <%= f.label :nickname %>
  <%= f.text_field :nickname %>
  <br>

  <%= f.label :nationality %>
  <%= f.text_field :nationality %>
  <br>

  <%= f.label :sliding_style %>
  <%= f.text_field :ski_type %>
  <br>

  <%= f.label :home_mountain %>
  <%= f.text_field :home_mountain %>
  <br>

  <%= f.label :mountains_shredded %>
  <%= f.text_field :mountains %>
  <br>

  <%= f.label :run_history %>
  <%= f.text_field :runs %>
  <br>


  <%= f.submit %>

<% end %>
