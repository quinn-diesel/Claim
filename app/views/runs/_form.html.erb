<%= form_for @run, html: {multipart: true} do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :length %>
  <%= f.number_field :length %>
  <br>

  <%= f.label :difficulty %>
  <%= f.text_field :difficulty %>
  <br>

  <%= f.label :number_of_deaths %>
  <%= f.number_field :number_of_deaths %>
  <br>

  <%= f.label :image %>
  <%= f.cl_image_upload :image %>
  <br>

  <%#= hidden_field_tag :mountain_id, :value => @mountain.id %> 

  <%= f.submit %>

<% end %>
